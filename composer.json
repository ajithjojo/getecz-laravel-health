{
  "name": "getecz/laravel-health",
  "description": "Laravel-native app health checks (DB/cache/queue/storage/cron) with dashboard + JSON endpoints.",
  "type": "library",
  "license": "MIT",
  "keywords": ["laravel", "health", "monitoring", "performance", "ops"],
  "authors": [
    {"name": "Ajith Joseph"}
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "orchestra/testbench": "^7.0|^8.0|^9.0|^10.0"
  },
  "autoload": {
    "psr-4": {
      "Getecz\\LaravelHealth\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Getecz\\LaravelHealth\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Getecz\\LaravelHealth\\LaravelHealthServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
